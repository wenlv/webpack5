{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": 0,
        "quotes": 0,
        "no-console": 0,
        "indent": [
            "error",
            4
        ],
        "import/no-unresolved": [
            2,
            {
                "commonjs": true,
                "amd": true,
                "ignore": [
                    "jquery"
                ]
            }
        ],
        "import/no-import-module-exports": 0,
        "no-restricted-globals": "off"
    }
}